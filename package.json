{
  "name": "perf-widget",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ftlabs/perf-widget.git"
  },
  "author": "FT Labs",
  "private": true,
  "license": "",
  "description": "Perf Widget front-end and back-end",
  "engines": {
    "node": "5.6.0"
  },
  "main": "server/bin/www",
  "dependencies": {
    "array-includes": "^3.0.1",
    "babel-core": "^6.3.21",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "bluebird": "^3.1.5",
    "body-parser": "^1.14.1",
    "bower": "^1.7.7",
    "chart.js": "^1.0.2",
    "debug": "^2.2.0",
    "denodeify": "^1.2.1",
    "dotenv": "^1.2.0",
    "express": "^4.13.3",
    "express-enforces-ssl": "^1.1.0",
    "express-ftwebservice": "^2.0.0",
    "express-hbs": "1.0.2",
    "graphite": "0.0.6",
    "gulp": "^3.9.0",
    "gulp-env": "^0.4.0",
    "gulp-insert": "^0.5.0",
    "gulp-preprocess": "^2.0.0",
    "gulp-uglify": "^1.5.1",
    "gulp-util": "^3.0.7",
    "hsts": "^1.0.0",
    "is-url-superb": "^2.0.0",
    "lodash": "^4.0.0",
    "lru-cache": "^4.0.0",
    "morgan": "^1.6.1",
    "mysql": "^2.10.2",
    "node-fetch": "^1.3.3",
    "node-ssllabs": "^0.4.3",
    "npm-run-all": "^1.4.0",
    "origami-build-tools": "5.0.0",
    "psi": "^2.0.2",
    "raven": "^0.9.0",
    "s3o-middleware": "^1.2.1",
    "yargs": "^3.32.0",
    "snyk": "^1.19.1"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "co": "^4.6.0",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4",
    "mockery": "^1.4.0",
    "nodemon": "^1.8.1",
    "sinon": "^1.17.2",
    "supertest": "^1.1.0"
  },
  "scripts": {
    "bower": "bower install",
    "build": "npm-run-all clean bower mkdir build:app build:extension build:client",
    "build:app": "gulp set-service-url",
    "build:client": "gulp client",
    "build:extension": "gulp build-extension",
    "build:extension:test": "gulp build-extension --destination TEST",
    "build:extension:live": "gulp build-extension --destination LIVE",
    "clean": "rimraf client/dist/ extension-dist/",
    "mkdir": "mkdir -p client/dist",
    "develop": "npm run watch -- --exec 'npm-run-all -p start build'",
    "lint": "obt verify --excludeFiles=\\!client/src/js/**/*.js",
    "postinstall": "npm run build",
    "start": "DEBUG=perf-widget:* node $npm_package_main",
    "tdd": "npm run watch -- --exec 'npm-run-all test'",
    "test": "DEBUG=perf-widget:* npm-run-all lint test:*",
    "test:integration": "npm-run-all test:integration:*",
    "test:integration:api": "_mocha ./tests/integration/api/*.spec.js",
    "test:server": "istanbul cover _mocha -- -R spec ./tests/server/**/*.spec.js",
    "watch": "nodemon --ignore client/dist --ignore coverage -e js,scss,json",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "snyk": true
}